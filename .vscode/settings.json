{
  "files.exclude": {
    "**/.git": true,
    "**/*.class": true
  },
  "files.encoding": "utf8",
  "files.eol": "\n",
  "editor.tabSize": 4,
  "code-runner.executorMap": {
    "java":
      "cd $dir && javac -encoding uTf8 $fileName && java -Dfile.encoding=utf8 $fileNameWithoutExt",
    "shellscript": "bash"
  },
}
